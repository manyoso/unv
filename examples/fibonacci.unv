/*
 * Example of calculating fibonacci in unv syntax.
 * http://en.wikipedia.org/wiki/Fibonacci_number
 */
alias Int _builtin_int32_

function main : () -> Int
    Int i = fibonacci(40)
    if (i == 102334155) return 0
    return 1

function fibonacci : (n:Int) -> Int
    if (n < 3) return 1
    return fibonacci(n - 1) + fibonacci(n - 2)
